   "source": [
    "import cv2\n",
    "src1 = cv2.imread('image1.png', cv2.IMREAD_COLOR)\n",
    "src2 = cv2.imread('image2.png', cv2.IMREAD_COLOR)\n",
    "\n",
    "# add or blend the images\n",
    "dst = cv2.addWeighted(src1, 0.5, src2, 0.5, 0.0)\n",
    "\n",
    "# save the output image\n",
    "cv2.imwrite('image.png', dst)"
   ]
  }
